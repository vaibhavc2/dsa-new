    "C_Cpp.dimInactiveRegions": false,
    "tabout.disableByDefault": false,
    "cSpell.enabled": false,
    "[cpp]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.formatOnType": false,
        "explicitFolding.rules": [
            {
                "begin": "// ---------- CP SETUP ---------- //",
                "end": "//*/*-------------- SOLUTION --------------*/*//",
                "autoFold": true
            },
            {
                "begin": "//*/*-------------- SOLUTIONS --------------*/*//",
                "end": "//*/*-------------- SOLUTIONS --------------*/*//",
                "autoFold": true
            },
            {
                "begin": "//*/*-------------- BigInt Implementation --------------*/*//",
                "end": "//*/*------------------- BigInt OVER -------------------*/*//",
                "autoFold": true
            },
            {
                "begin": "// ************* IO Implementations *************",
                "end": "// !! xxxxxxxx !! START FROM HERE !! xxxxxxxx !!",
                "autoFold": true
            },
            {
                "begin": "int32_t main() {",
                "end": "// ------------- END -------------",
                "autoFold": true
            },
            {
                "begin": "int main() {",
                "end": "// ------------- END -------------",
                "autoFold": true
            },
            {
                "begin": "/*",
                "end": " */",
                "autoFold": true
            },
            {
                "begin": "// ------------- OPTIMIZATIONS -------------",
                "end": "// ------------- CODE -------------",
                "autoFold": true
            },
            {
                "beginRegex": "#if(?:n?def)?",
                "middleRegex": "#el(?:se|if)",
                "endRegex": "#endif",
                "autoFold": false
            }
        ],
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[plaintext]": {
        "editor.fontSize": 12,
        "editor.fontFamily": "'SF Mono', monospace",
        "editor.lineHeight": 0,
        "editor.minimap.enabled": false,
        "editor.glyphMargin": false,
        "editor.wordWrap": "off"
    },
    "[Log]": {
        "editor.fontSize": 12,
        "editor.fontFamily": "'SF Mono', monospace",
        "editor.lineHeight": 0,
        "editor.minimap.enabled": false,
        "editor.glyphMargin": false,
        "editor.wordWrap": "on"
    },
    "workbench.colorTheme": "Default Dark Modern",
    "editor.wordWrap": "on",
    "breadcrumbs.enabled": false,
    "task.autoDetect": "off",
    "C_Cpp.default.cppStandard": "c++20"
